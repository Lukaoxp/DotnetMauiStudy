<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="MauiNavigation.Pages.Pagina2"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="Pagina2">
    <VerticalStackLayout HorizontalOptions="Center" VerticalOptions="Center">
        <Button
            x:Name="NavigateToFirstPageButton"
            Clicked="NavigateToFirstPageButton_Clicked"
            Text="Back to start page" />
        <Button
            x:Name="NavigateToLastPageButton"
            Clicked="NavigateToLastPageButton_Clicked"
            Text="To Last Page" />
    </VerticalStackLayout>
</ContentPage>